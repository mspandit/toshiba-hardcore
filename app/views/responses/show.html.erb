<table>
	<tr>
		<% [:final_score, :name, :speed, :memory, :graphics, :hard, :hdd_speed, :optical, :price, :screen].each do |attribute| %>
			<th>
				<%= attribute %>
			</th>
		<% end %>
	</tr>
	<% Item.rank_all(@response).each do |item| %>
		<tr>
		<% [:final_score, :name, :speed, :memory, :graphics, :hard, :hdd_speed, :optical, :price, :screen].each do |attribute| %>
			<td>
				<%= item.send(attribute) %>
			</td>
		<% end %>
		</tr>
	<% end %>
</table>

